{
    "nodes": [
        {
            "node": "kinase",
            "example": [
                "CDC7",
                "MAST4"
            ],
            "display": [
                {
                    "label": "kinase ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ]
        },
        {
            "node": "kinase phosphosite",
            "example": [
                "AAK1_S509",
                "AAK1_T1389"
            ],
            "display": [
                {
                    "label": "kinase phosphosite ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ]
        }
    ],
    "edges": [
        {
            "match": [
                "phosphosite"
            ],
            "selected": true,
            "display": [
                {
                    "label": "source",
                    "text": "${source}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target",
                    "text": "${target}",
                    "type": "text"
                },
                {
                    "label": "source label",
                    "text": "${source_label}",
                    "type": "text"
                },
                {
                    "label": "target label",
                    "text": "${target_label}",
                    "type": "text"
                }
            ]
        },
        {
            "match": [
                "phosphorylates"
            ],
            "selected": true,
            "display": [
                {
                    "label": "source",
                    "text": "${source}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target",
                    "text": "${target}",
                    "type": "text"
                },
                {
                    "label": "source label",
                    "text": "${source_label}",
                    "type": "text"
                },
                {
                    "label": "target label",
                    "text": "${target_label}",
                    "type": "text"
                },
                {
                    "label": "percentile",
                    "text": "${percentile}",
                    "type": "text"
                }
            ]
        }
    ],
    "header": {
        "divider": true,
        "header": "KEA-KG",
        "title": "KEA-KG",
        "counter": true,
        "counterTop": true,
        "background": {
            "backgroundColor": "#000",
            "contrastText": "#FFF"
        },
        "icon": {
            "src": "/hgrnchear_logo.png",
            "favicon": "/hgrnchear_logo.png",
            "faviconTitle": "KEA-KG",
            "alt": "KEA-KG",
            "key": "hGRN_ChEAR_icon",
            "width": 200,
            "height": 120,
            "avatar": false
        },
        "tabs": [
            {
                "endpoint": "/",
                "label": "Search",
                "type": "page",
                "component": "KnowledgeGraph",
                "props": {
                    "init_function": "initialize_kg",
                    "title": "Kinase-Kinase Regulatory Subnetworks",
                    "description": "Search for subnetworks within the KEA3 GRN by entering one or two kinases. The background GRN contains 42,322 signed and directed kinase-kinase regulatory relationships between 20,659 source and target human kinases and phosphosites.",
                    "initial_query": {
                        "start": "Kinase",
                        "start_term": "CDC7",
                        "start_field": "source_label"
                    },
                    "subheader": {
                        "url_field": "filter",
                        "query_field": "relation"
                    }
                }
            },
            {
                "endpoint": "/enrichment",
                "label": "Enrichment",
                "type": "page",
                "component": "KEA3Enrichment",
                "props": {
                    "init_function": "initialize_enrichment",
                    "title": "Kinase Enrichment Analysis with KEA3",
                    "description": "Enter a set of Entrez gene below to perform enrichment analysis using [KEA3](https://maayanlab.cloud/kea3/).",
                    "placeholder": "Enter newline separated gene sets",
                    "disableHeader": true,
                    "disableLibraryLimit": true,
                    "libraries": [
                        {
                            "name": "Integrated--meanRank",
                            "node": "Kinase"
                        }
                    ],
                    "sortLibraries": false,
                    "default_options": {
                        "min_lib": 3,
                        "libraries": [
                            {
                                "name": "Integrated--meanRank",
                                "limit": 10
                            }
                        ]
                    },
                    "subheader": {
                        "url_field": "q",
                        "query_field": "libraries"
                    }
                }
            }
        ]
    },
    "footer": {
        "style": {
            "background": "#1F3D5C",
            "color": "#FFF"
        },
        "layout": [
            [
                {
                    "component": "logo",
                    "props": {
                        "src": "/hgrnchear_white.png",
                        "alt": "hGRN_ChEAR_logo",
                        "title": "KEA-KG",
                        "color": "inherit",
                        "size": "medium"
                    }
                },
                {
                    "component": "github",
                    "props": {
                        "code": "https://github.com/MaayanLab/ChEA-KG-UI",
                        "issues": "https://github.com/MaayanLab/TranscriptionNetwork-Demo/issues"
                    }
                }
            ],
            [
                {
                    "component": "icon",
                    "props": {
                        "type": "icon",
                        "key": "lab_icon",
                        "src": "https://minio.dev.maayanlab.cloud/enrichr-kg/maayanlab.png",
                        "alt": "MaayanLab",
                        "href": "https://labs.icahn.mssm.edu/maayanlab/",
                        "height": 60,
                        "width": 150
                    }
                }
            ],
            [
                {
                    "component": "icon",
                    "props": {
                        "type": "icon",
                        "key": "center_icon",
                        "src": "https://minio.dev.maayanlab.cloud/enrichr-kg/bioinformatics.png",
                        "alt": "COB",
                        "href": "https://icahn.mssm.edu/research/bioinformatics",
                        "height": 60,
                        "width": 150
                    }
                }
            ],
            [
                {
                    "component": "license",
                    "props": {
                        "type": "icon",
                        "key": "center_icon",
                        "src": "https://s3.amazonaws.com/maayan-kg/chea-kg/gpl3.png",
                        "alt": "gpl3",
                        "href": "https://www.gnu.org/licenses/gpl-3.0.en.html",
                        "height": 60,
                        "width": 150
                    }
                }
            ]
        ],
        "footer_text": "<span style='color:white'>This website was prepared using KG UI. KG UI is being developed by the Ma'ayan Lab at Icahn School of Medicine and is available <a style='color: #81A1C1' href='https://github.com/MaayanLab/KnowledgeGraphUIDemo'>here</a>.</span>"
    },
    "ui_theme": "enrichr_kg_theme"
}