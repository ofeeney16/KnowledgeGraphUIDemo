{
    "nodes": [
        {
            "node": "kinase",
            "example": [
                "CDC7",
                "MAST4"
            ],
            "display": [
                {
                    "label": "Kinase",
                    "text": "${label}",
                    "type": "text"
                },
                {
                    "label": "Harmonizome",
                    "href": "https://maayanlab.cloud/Harmonizome/gene/${label}",
                    "text": "${label}",
                    "type": "link"
                },
                {
                    "label": "ARCHS4",
                    "href": "https://maayanlab.cloud/archs4/gene/${label}",
                    "text": "${label}",
                    "type": "link"
                },
                {
                    "label": "GDLPA",
                    "href": "https://cfde-gene-pages.cloud/gene/${label}?CF=false&PS=true",
                    "text": "${label}",
                    "type": "link"
                },
                {
                    "label": "NCBI Gene",
                    "href": "https://www.ncbi.nlm.nih.gov/gene/${id.replace('NCBI:', '')}",
                    "text": "${id}",
                    "type": "link"
                }
            ],
            "relation": [],
            "search": [
                "label"
            ],
            "color": "#80D8FF"
        },
        {
            "node": "kinase_phosphosite",
            "example": [
                "AAK1_T23",
                "AKT2_S888"
            ],
            "display": [
                {
                    "label": "Kinase Phosphosite",
                    "text": "${label}",
                    "type": "text"
                }
            ],
            "relation": [],
            "search": [
                "label"
            ],
            "color": "#00caff"
        }
    ],
    "edges": [
        {
            "match": [
                "phosphorylates"
            ],
            "directed": "triangle",
            "display": [
                {
                    "label": "source label",
                    "text": "${source_label}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target label",
                    "text": "${target_label}",
                    "type": "text"
                },
                {
                    "label": "z score",
                    "text": "${z_score}",
                    "type": "text"
                },
                {
                    "label": "p value",
                    "text": "${p_value}",
                    "type": "text"
                }
            ]
        },
        {
            "match": [
                "phosphosite"
            ],
            "color": "#c70a2d",
            "directed": "tee",
            "display": [
                {
                    "label": "source label",
                    "text": "${source_label}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target label",
                    "text": "${target_label}",
                    "type": "text"
                },
                {
                    "label": "z score",
                    "text": "${z_score}",
                    "type": "text"
                },
                {
                    "label": "p value",
                    "text": "${p_value}",
                    "type": "text"
                }
            ]
        }
    ],
    "header": {
        "divider": true,
        "header": "KEA-KG",
        "title": "KEA-KG",
        "counter": true,
        "counterTop": true,
        "background": {
            "backgroundColor": "#000",
            "contrastText": "#FFF"
        },
        "icon": {
            "src": "/hgrnchear_logo.png",
            "favicon": "/hgrnchear_logo.png",
            "faviconTitle": "KEA-KG",
            "alt": "KEA-KG",
            "key": "hGRN_ChEAR_icon",
            "width": 200,
            "height": 120,
            "avatar": false
        },
        "tabs": [
            {
                "endpoint": "/",
                "label": "Search",
                "type": "page",
                "component": "KnowledgeGraph",
                "props": {
                    "init_function": "initialize_kg",
                    "title": "Kinase Regulatory Networks",
                    "description": "",
                    "initial_query": {
                        "start": "kinase",
                        "start_term": "MAPK1",
                        "start_field": "label"
                    },
                    "subheader": {
                        "url_field": "filter",
                        "query_field": "relation"
                    }
                }
            },
            {
                "endpoint":"/enrichment",
                "label":"Enrichment",
                "type":"page",
                "component":"Enrichment",
                "props": {
                    "init_function": "initialize_enrichment",
                    "title": "Kinase Enrichment Analysis with KEA3",
                    "description": "Enter a set of Entrez phosphorylated proteins below to perform enrichment analysis using [KEA3](https://maayanlab.cloud/kea3/).",
                    "placeholder": "Enter newline separated protein sets",
                    "disableHeader": true,
                    "disableLibraryLimit": true,
                    "example": {
                        "gene_set": "ZNF264\nTMPO\nISL2\nMAP3K8\nEFNB1\nEIF3C\nOSBPL11\nABCF1\nUTRN\nOPRK1\nTSC1\nGAB2\nRPS3P2\nDDX3X\nPPP1CA\nNF2\nRBM3\nIRAK1\nKCNH2\nNPR1\nMOCOS\nITSN2\nMITF\nARAF\nDAPK2\nEPHB2\nCACNA1G\nYWHAZ\nGMFB",
                        "description": "Sample",
                        "libraries": [
                            {
                                "name": "Integrated--meanRank",
                                "limit": 10
                            }
                        ],
                        "gene_degree": 3
                    },
                    "libraries": [
                        {
                            "name": "Integrated--meanRank",
                            "node": "kinase"
                        }
                    ],
                    "sortLibraries": false,
                    "default_options": {
                        "min_lib": 3,
                        "libraries": [
                            {
                                "name": "Integrated--meanRank",
                                "limit": 10
                            }
                        ]
                    },
                    "subheader": {
                        "url_field": "q",
                        "query_field": "libraries"
                    }
                }
            }
        ]
    },
    "footer": {
        "style": {
            "background": "#1F3D5C",
            "color": "#FFF"
        },
        "layout": [
            [
                {
                    "component": "logo",
                    "props": {
                        "src": "/hgrnchear_white.png",
                        "alt": "hGRN_ChEAR_logo",
                        "title": "KEA-KG",
                        "color": "inherit",
                        "size": "medium"
                    }
                },
                {
                    "component": "github",
                    "props": {
                        "code": "https://github.com/MaayanLab/ChEA-KG-UI",
                        "issues": "https://github.com/MaayanLab/TranscriptionNetwork-Demo/issues"
                    }
                }
            ],
            [
                {
                    "component": "icon",
                    "props": {
                        "type": "icon",
                        "key": "lab_icon",
                        "src": "https://minio.dev.maayanlab.cloud/enrichr-kg/maayanlab.png",
                        "alt": "MaayanLab",
                        "href": "https://labs.icahn.mssm.edu/maayanlab/",
                        "height": 60,
                        "width": 150
                    }
                }
            ],
            [
                {
                    "component": "icon",
                    "props": {
                        "type": "icon",
                        "key": "center_icon",
                        "src": "https://minio.dev.maayanlab.cloud/enrichr-kg/bioinformatics.png",
                        "alt": "COB",
                        "href": "https://icahn.mssm.edu/research/bioinformatics",
                        "height": 60,
                        "width": 150
                    }
                }
            ],
            [
                {
                    "component": "license",
                    "props": {
                        "type": "icon",
                        "key": "center_icon",
                        "src": "https://s3.amazonaws.com/maayan-kg/chea-kg/gpl3.png",
                        "alt": "gpl3",
                        "href": "https://www.gnu.org/licenses/gpl-3.0.en.html",
                        "height": 60,
                        "width": 150
                    }
                }
            ]
        ],
        "footer_text": "<span style='color:white'>This website was prepared using KG UI. KG UI is being developed by the Ma'ayan Lab at Icahn School of Medicine and is available <a style='color: #81A1C1' href='https://github.com/MaayanLab/KnowledgeGraphUIDemo'>here</a>.</span>"
    },
    "ui_theme": "cfde_theme"
}